{
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"es6": true,
		"jest": true,
		"node": true
	},
	"extends": ["eslint:recommended"<% if (tech.react) { %>, "plugin:react/recommended"<% } %>],
	"rules": {
		"no-console": 0
	}
}
